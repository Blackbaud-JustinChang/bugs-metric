Enter New Graph information
<hr>

<%= form_for @graph do |g| -%>
    Graph Name: <%= g.text_field :name %><br />
    Search: <%= g.text_field :search %><br />
    <%= g.submit("Add graph") %>
<% end -%>
<hr>
Display Current Graphs <br />
<%= render @graphs %>
<hr>
<% products = [["Luminate Online", "1"], ["Luminate CRM", "2"], ["BBCRM Integration", "3"]] %>
Product: <%= select_tag(:product_id, options_for_select(products)) %> <br>
Start Date <%= date_select(:start_date, :start_year => 1940, discard_day:true) %> <br>
End Date <%= date_select(:end_date, :end_year => 1940, discard_day:true) %><br>
<%= button_to "Graph", {:controller => 'graphs', :action => "show_graph"}, :method => "post"%>




