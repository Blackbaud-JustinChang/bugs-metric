<!DOCTYPE html>
<html>
<head>
  <title>BugsMetric</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body id="background">

    <div id="Main">
        <% flash.each do |name, msg| %>
            <%= content_tag :div, msg, class: name %>
        <% end %>

        <%= yield %>
    </div>

</body>
</html>
